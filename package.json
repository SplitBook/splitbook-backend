{
  "name": "SplitBook",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "npm-run-all -p start:*",
    "start:server": "node src/server.js",
    "start:queue": "node src/queue.js",
    "db": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "db:rollback": "knex migrate:rollback",
    "dev": "npm-run-all -p dev:*",
    "dev:server": "tsnd -r tsconfig-paths/register --respawn --ignore-watch node_modules src-typescript/server.ts",
    "dev:queue": "tsnd -r tsconfig-paths/register --respawn --ignore-watch node_modules src-typescript/queue.ts"
  },
  "author": "Nuno Alves",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bull": "^3.14.0",
    "celebrate": "^12.1.1",
    "cors": "^2.8.5",
    "csv-parser": "^2.3.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "html-pdf": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.6",
    "pg": "^8.6.0",
    "pug": "^2.0.4",
    "redis": "^3.0.2",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/knex": "^0.16.1",
    "@types/multer": "^1.4.6",
    "@types/node": "^15.12.5",
    "bull-board": "^1.7.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.4"
  }
}
